  "schemas": [
    {
      "title": "people",
      "type": "array",
      "format": "tabs",
      "items": {
        "title": "Person",
        "headerTemplate": "{{self.name}}",
        "type": "object",
        "id": "person",
        "properties": {
          "name": {
            "type": "string",
            "default": "{{_fullname 'ru' 'ja' 'es'}}",
            "description": "First and Last name",
            "minLength": 4
          },
          "age": {
            "type": "integer",
            "default": "{{_random 18 99}}",
            "minimum": 18,
            "maximum": 99
          },
          "gender": {
            "type": "string",
            "enum": [
              "male",
              "female"
            ]
          },
          "location": {
            "type": "object",
            "title": "Location",
            "properties": {
              "city": {
                "type": "string",
                "default": "{{_city}}"
              },
              "state": {
                "type": "string",
                "default": "{{_state}}"
              },
              "citystate": {
                "type": "string",
                "description": "This is generated automatically from the previous two fields",
                "template": "{{city}}, {{state}}",
                "watch": {
                  "city": "person.location.city",
                  "state": "person.location.state"
                }
              }
            }
          },
          "pets": {
            "type": "array",
            "format": "tabs",
            "title": "Pets",
            "uniqueItems": true,
            "items": {
              "type": "object",
              "headerTemplate": "{{self.name}} the {{self.type}}",
              "properties": {
                "type": {
                  "type": "string",
                  "enum": [
                    "cat",
                    "dog",
                    "bird",
                    "reptile",
                    "other"
                  ],
                  "default": "{{_random 'cat' 'dog' 'bird' 'reptile'}}"
                },
                "name": {
                  "type": "string",
                  "default": "{{_firstname}}"
                },
                "fixed": {
                  "type": "boolean",
                  "title": "spayed / neutered"
                }
              }
            }
          }
        }
      },
      "id": 1
    },
    {
      "title": "posts",
      "type": "array",
      "format": "tabs",
      "items": {
        "title": "Article",
        "id": "article",
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "default": "{{_fullname}}"
          },
          "body": {
            "type": "string",
            "default": "{{_fullname}}"
          },
          "id": {
            "type": "integer"
          }
        }
      },
      "id": 2
    },
    {
      "title": "pets",
      "type": "array",
      "format": "tabs",
      "items": {
        "title": "Pet",
        "id": "pet",
        "type": "object",
        "headerTemplate": "{{self.type}}",
        "properties": {
          "name": {
            "type": "string",
            "default": "{{_firstname}}"
          },
          "color": {
            "type": "string",
            "format": "color"
          },
          "type": {
            "type": "string",
            "enum": [
              "cat",
              "dog",
              "horse",
              "pig",
              "turtle",
              "fish",
              "bird",
              "spider",
              "snake",
              "mouse"
            ],
            "default": "{{_random 'cat' 'dog' 'horse' 'pig' 'turtle' 'fish' 'bird' 'spider' 'snake' 'mouse'}}"
          },
          "postId": {
            "type": "integer",
            "default": "{{_random 1 6}}"
          }
        }
      },
      "id": 3
    }
  ],
